PREFIX was: <https://was-course.interactions.ics.unisg.ch/farm-ontology#>
PREFIX hmas: <https://purl.org/hmas/>

SELECT ?coordinates WHERE {
GRAPH <https://sandbox-graphdb.interactions.ics.unisg.ch/was-exercise-3-xuemei#> {
   bind (<https://sandbox-graphdb.interactions.ics.unisg.ch/was-exercise-3-danai#farm-17c04810-567a-4236-b310-611bb4fd2a8c> as ?farm)
   ?farm a was:Farm.    
   ?farm was:hasLandSection ?section.
   ?section a was:Section.
   ?section was:hasCoordinates ?cooridnates.
   ?cooridnate a was:Coordinates.
 }
}